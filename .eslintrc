{
  "extends": "airbnb",
  "rules": {
    "indent": 2,
    "func-names": "off",
    "no-unused-expressions": "off",
    "no-new": "off",
    "class-methods-use-this": "off"
  },
  "globals": {
    "describe": true,
    "it": false,
    "beforeEach": true,
    "before": true,
    "xit": false,
    "afterEach": true,
    "mountWithIntl": true,
    "shallowWithIntl": true
  }
}
